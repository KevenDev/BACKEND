// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User{
  id Int @id @default(autoincrement())
  name String
  setor Setor @relation(fields: [fk_id_setor], references: [id])
  fk_id_setor Int @unique

  Telefone Telefone[]

   @@map("user")
}

model Telefone{
  id Int @id @default(autoincrement())
  user User @relation(fields: [fk_id_user], references: [id])
  setor Setor @relation(fields: [fk_id_setor], references: [id])
  fk_id_user Int
  fk_id_setor Int

  @@map("telefone")
}



model Setor{
  id Int @id @default(autoincrement())
  name String
  user User?
  Telefone Telefone[]
  @@map("setor")

}

